<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.metoak.mes.mapper.MoPackingInfoMapper">

    <delete id="deleteByPackingCode">
        DELETE
        FROM
            mo_packing_info
        WHERE
            packing_code = #{ packingCode }

    </delete>
    <delete id="deleteByCameraSn">
        DELETE
        FROM
            mo_packing_info
        WHERE
            camera_sn = #{ cameraSn }
    </delete>
</mapper>
